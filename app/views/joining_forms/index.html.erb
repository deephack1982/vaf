<h1 class="text-center">Listing Joining Forms</h1>

<table>
  <thead>
    <tr>
      <th>Age</th>
      <th>English proficiency</th>
      <th>Available</th>
      <th>Software</th>
      <th>Years experience</th>
      <th>Interests</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @joining_forms.each do |joining_form| %>
      <tr>
        <td><%= joining_form.age %></td>
        <td><%= joining_form.english_proficiency %></td>
        <td><%= joining_form.available %></td>
        <td><%= joining_form.software %></td>
        <td><%= joining_form.years_experience %></td>
        <td><%= joining_form.interests %></td>
        <td><%= joining_form.notes %></td>
        <td><%= link_to 'Show', joining_form %></td>
        <td><%= link_to 'Edit', edit_joining_form_path(joining_form) %></td>
        <td><%= link_to 'Destroy', joining_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= button_to 'New Joining form', new_joining_form_path, method: :get, :class => "button success" %>
