<h1 class="text-center">Listing Qualifications</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qualifications.each do |qualification| %>
      <tr>
        <td><%= qualification.tag %></td>
        <td><%= qualification.name %></td>
        <td><%= button_to 'Show', qualification, method: :get, :class => "button tiny" %></td>
        <td><%= button_to 'Edit', edit_qualification_path(qualification), method: :get, :class => "button tiny success" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= button_to 'New Qualification', new_qualification_path, method: :get, :class => "button success" %>
